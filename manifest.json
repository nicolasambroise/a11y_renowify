{
  "manifest_version": 3,
  "name": "Renowify - Check Access",
  "description": "Vérification de l'accessibilité des sites web",
  "version": "2.2",
  "author": "Nicolas AMBROISE",
  "icons": {
    "16": "images/renow-16.png",
    "32": "images/renow-32.png",
    "48": "images/renow-48.png",
    "144": "images/renow-144.png",
    "180": "images/renow-180.png"
  },
  "action": {
    "default_title": "Renowify",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/renow-16.png",
      "32": "images/renow-32.png",
      "48": "images/renow-48.png",
      "144": "images/renow-144.png",
      "180": "images/renow-180.png"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": false
  },
  "permissions": ["scripting", "activeTab", "storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Insert",
        "mac": "Command+Insert"
      }
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "noreply@renowify.lu",
      "strict_min_version": "109.0"
    }
  }
}
